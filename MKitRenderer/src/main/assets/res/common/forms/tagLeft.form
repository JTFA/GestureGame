	alignment = ALIGN_LEFT
	layout = LAYOUT_FLOW
	size = 620, 130

	image image1
	{
		alignment = ALIGN_VCENTER
		path = res/png/pointBlue.png
		size = 50, 50
	}

	image image2
	{
		alignment = ALIGN_VCENTER
		path = res/png/lineBlue.png
		size = 40, 15
	}

	container cover
	{
		layout = LAYOUT_ABSOLUTE
